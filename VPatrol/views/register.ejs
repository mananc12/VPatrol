<%- include('partials/header') %>

<div>
    <form action="/submit" method="POST">
        <h1>You have to register first</h1>
        <label>Username</label>
        <input type="text" name="user_name" id=""><br>
        <label>Name</label>
        <input type="text" name="name" id=""><br>
        <label>ID</label>
        <input type="number" name="user_id" id=""><br>
        <label>Bank accounts (comma-separated)</label>
<input name="bank_accounts" type="text" placeholder="Account1, Account2, Account3" />
<br>
        <div>
        <h2>Accounts</h2>
        <label>IFSC</label>
        <input type="text" name="ifsc" id="ifscInput">
        <label>Bank</label>
        <input type="text" name="bank" id="bankInput"><br>
        <label>Branch</label>
        <input type="text" name="branch" id="branchInput"><br>
        <label>Address</label>
        <input type="text" name="address" id="addressInput"><br>
        <label>City</label>
        <input type="text" name="city" id="cityInput"><br>
        <label>District</label>
        <input type="text" name="district" id="districtInput"><br>
        <label>State</label>
        <input type="text" name="state" id="stateInput"><br>
        <label>Bank Code</label>
        <input type="text" name="bank_code" id="bankCodeInput"><br>
        <div>
        <h3>Weather</h3>
<label>Temperature</label>
        <input type="text" name="temperature" id="temperatureInput">
        <label>Humidity</label>
        <input type="text" name="humidity" id="humidityInput">
    </div>
</div>
        <button type="submit" name="submit">Submit</button>
    </form>
</div>

